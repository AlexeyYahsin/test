'use strict'

let numberOfFilms = +prompt('Сколько фильмов вы уже посмотрели?', '');

const personalMovieDB = {
    count : '',
    movie : {},
    actors : {},
    genres : [],
    privat : false
};

personalMovieDB.count = numberOfFilms;

function prompts() {
    let lastFilms = prompt('Один из последних просмотренных фильмов?', '');
    let filmRating = prompt('На сколько оцените его?', '');

    personalMovieDB.movie[lastFilms] = filmRating;    
}

function cycle(foo) {
    for (let i = 0; i < 2; i++){
       prompts()
    }
}

cycle();